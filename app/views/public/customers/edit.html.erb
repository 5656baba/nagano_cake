<h1>会員情報編集</h1>
<%= form_with model: @customer, url: customers_path, local:true do |f| %>
<table>
  <tr>
    <th>氏名</th>
    <th><%= f.text_field :last_name %></th>
    <th><%= f.text_field :first_name %></th>
  </tr>
  <tr>
    <th>カナ</th>
    <th><%= f.text_field :last_name_kana %></th>
    <th><%= f.text_field :first_name_kana %></th>
  </tr>
  <tr>
    <th>郵便番号</th>
    <th><%= f.text_field :postal_code %></th>
  </tr>
  <tr>
    <th>住所</th>
    <th><%= f.text_field :address %></th>
  </tr>
  <tr>
    <th>電話番号</th>
    <th><%= f.text_field :telephone_number %></th>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <th><%= f.text_field :email %></th>
  </tr>
</table>
<table>
  <tr>
    <th></th>
    <th>
      <button type="submit" class="btn btn-primary">編集内容をを保存</button>
    </th>
    <th></th>
    <th>
      <%= link_to "退会する",unsubscribe_customers_path %>
    </th>
  </tr>
</table>
<% end %>